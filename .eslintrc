{
  "extends": [
    // "prettier",
    // "./node_modules/eslint-config-react-app/index.js",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "class-property"
  ],
  "rules": {
    "react/jsx-closing-tag-location":"off",
    "max-len":0,
    "linebreak-style": 0,
    "quotes": [
      "error",
      "single"
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "jsx-a11y/href-no-hash": "off",
    "react/jsx-closing-bracket-location": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "object-curly-newline": "off",
    "arrow-parens": [
      "off"
    ]
  },
  "env": {
    "browser": true
  }
}
